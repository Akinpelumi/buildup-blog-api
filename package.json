{
  "name": "class-authentication-authorization",
  "version": "1.0.0",
  "description": "authentication & authorization",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset:test && cross-env NODE_ENV=test npm run migrate:up:test",
    "test": "cross-env NODE_ENV=test debug=true nyc --reporter=lcov --reporter=text --reporter=text-summary mocha --timeout 120000 test/index.js --exit",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "seedAdmin": "node src/seedAdmin.js",
    "migrate:create": "db-migrate create",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down",
    "migrate:up:test": "db-migrate up --config database.json -e test",
    "migrate:reset:test": "db-migrate reset --config database.json -e test"
  },
  "keywords": [
    "authenticate",
    "authorise"
  ],
  "author": "BuildUp Backend Cohort one",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.4",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.8.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-fileupload": "^1.5.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "pg-promise": "^12.3.0",
    "postmark": "^4.0.5",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.8",
    "chai-http": "^4.4.0",
    "cross-env": "^10.1.0",
    "mocha": "^5.0.5",
    "nodemon": "^3.1.11",
    "nyc": "^17.1.0",
    "sinon": "^21.0.1"
  }
}
